{
  "name": "apidoc-api-builder",
  "version": "0.0.2",
  "description": "Using apidoc to generate typescript or kotlin code",
  "main": "index.ts",
  "scripts": {
    "build": "tsc --declaration",
    "test1": "ts-node ./bin/cli.ts -t ts -s D:/project/node/botserver/apidoc/api_data.json -d D:/project/node/oxox2/src/js/api.ts",
    "test2": "apibuilder -t ts -s D:/project/node/botserver/apidoc/api_data.json -d D:/project/node/oxox2/src/js/api.ts"
  },
  "files": [
    "lib/**/*"
  ],
  "bin": {
    "apibuilder": "bin/cli.js"
  },
  "keywords": [
    "apidoc"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/yargs": "^17.0.2"
  },
  "dependencies": {
    "yargs": "^17.0.1"
  }
}
